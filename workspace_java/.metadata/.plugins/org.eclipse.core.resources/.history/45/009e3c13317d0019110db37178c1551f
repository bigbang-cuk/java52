package com.quiz0523;

import java.util.Random;

public class Test1 {
	int a[] = new int[3];
	public void orderBy() {
		
	}
	public void arrayInit() {
		Random r = new Random();
		for(int i=0;i<a.length;i++) {
			a[i]=r.nextInt(21);
		}
	}
	public void arrayPrint() {
		for(int i=0;i<a.length;i++) {
			System.out.println(a[i]);
		}		
	}
	public static void main(String[] args) {
		Test1 t1 = new Test1();
		t1.arrayInit();
		t1.arrayPrint();
	}

}
