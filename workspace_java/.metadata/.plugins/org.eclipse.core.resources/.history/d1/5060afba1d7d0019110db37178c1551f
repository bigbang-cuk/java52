package com.ch7;

public class ParameterTypeA {
	void methodA(int jumsu[]){
		
	}
	public static void main(String[] args) {
	//insert here - 배열선언 및 초기화 처리
		int jumsu[] = null;
		jumsu = new int[3];
		jumsu[0] = 70;
		jumsu[1] = 85;
		jumsu[2] = 95;
		ParameterTypeA pta = new ParameterTypeA();
		pta.methodA(jumsu);
	}

}
